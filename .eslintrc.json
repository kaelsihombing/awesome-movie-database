{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  },
  "ignorePatterns": [
    "exceptionHandler.js",
    "userSpec.js",
    "movieSpec.js",
    "incumbentSpec.js",
    "reviewSpec.js",
    "review.spec.js",
    "movie.spec.js",
    "user.spec.js",
    "incumbent.spec.js",
    "jest.config.js"
  ]
}
